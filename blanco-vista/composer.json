{
	"name": "starbase/spacestation",
	"type": "project",
	"license": "MIT",
	"description": "A modern WordPress stack",
	"homepage": "https://github.com/star-base/spacestation",
	"authors": [
		{
			"name": "Brett Bash",
			"email": "brettbash@brettbash.com",
			"homepage": "https://github.com/brettbash"
		}
	],
	"support": {
		"issues": "https://github.com/star-base/spacestation/issues"
	},
	"config": {
		"preferred-install": "dist"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"require": {
		"php": ">=5.5",
		"composer/installers": "~1.0.12",
		"vlucas/phpdotenv": "^2.0.1",
		"johnpbloch/wordpress": "4.4.2",
		"oscarotero/env": "^1.0",
		"roots/wp-password-bcrypt": "1.0.0",
		"wpackagist-plugin/advanced-custom-fields":"^4.4.5",
		"wpackagist-plugin/wordpress-seo":"^3.1.1",
		"wpackagist-plugin/better-wp-security":"^5.3.4",
		"roots/soil":"^3.7.0",
		"roots/wp-stage-switcher": "2.0.0",
		"oscarotero/inline-svg": "^1.0"
	},
	"extra": {
		"installer-paths": {
			"web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
			"web/app/plugins/{$name}/": ["type:wordpress-plugin"],
			"web/app/themes/{$name}/": ["type:wordpress-theme"]
		},
		"wordpress-install-dir": "web/wp"
	}
}
